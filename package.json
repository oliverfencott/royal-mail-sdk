{
  "name": "royal-mail-sdk",
  "version": "1.0.0",
  "description": "Autogenerated SDK for Royal Mail from OpenAPI",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "generate": "run-p generate:ts-axios",
    "generate:ts-axios": "openapi-generator generate --additional-properties=npmName=royal-mail-sdk -i api-spec.yaml -g typescript-axios -o src",
    "push": "npm run generate && cd src && npm run publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oliverfencott/royal-mail-sdk.git"
  },
  "author": "Oliver Fencott",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oliverfencott/royal-mail-sdk/issues"
  },
  "homepage": "https://github.com/oliverfencott/royal-mail-sdk#readme",
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^1.0.15-4.3.1",
    "esbuild": "^0.6.28",
    "npm-run-all": "^4.1.5"
  }
}